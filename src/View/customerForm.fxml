<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.customerForm">
   <children>
      <TableView layoutX="65.0" layoutY="65.0" prefHeight="470.0" prefWidth="870.0">
        <columns>
          <TableColumn editable="false" prefWidth="49.0" text="ID" />
          <TableColumn editable="false" prefWidth="140.0" text=" Name" />
            <TableColumn editable="false" prefWidth="174.0" text="Address" />
            <TableColumn editable="false" minWidth="3.0" prefWidth="123.0" text="Postal Code" />
            <TableColumn editable="false" prefWidth="69.0" text="Phone" />
            <TableColumn editable="false" prefWidth="94.0" text="Divison ID" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Label layoutX="416.0" layoutY="6.0" text="Customers">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Button fx:id="addButton" layoutX="180.0" layoutY="552.0" mnemonicParsing="false" onAction="#onAdd" prefHeight="25.0" prefWidth="55.0" text="Add" />
      <Button fx:id="updateButton" layoutX="470.0" layoutY="552.0" mnemonicParsing="false" onAction="#onUpdate" text="Update" />
      <Button fx:id="deleteButton" layoutX="775.0" layoutY="552.0" mnemonicParsing="false" onAction="#onDelete" prefHeight="25.0" prefWidth="55.0" text="Delete" />
      <Button fx:id="apptButton" layoutX="65.0" layoutY="33.0" mnemonicParsing="false" onAction="#onSelect" text="Appointments" />
   </children>
</AnchorPane>
